<!-- Modal -->
<div class="modal fade" id="edit-modal-feature" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
<div class="modal-dialog">
    <div class="modal-content">
        <div class="modal-header">
            <h5 class="modal-title" id="exampleModalLabel">Update Feature</h5>
            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                <span aria-hidden="true">&times;</span>
            </button>
        </div>
        <form action="/admin/item/update/feature?_method=PUT" method="POST" enctype="multipart/form-data">
            <div class="modal-body">
                <div class="form-group">
                    <label for="name">Nama Bank</label>
                    <input type="text" class="form-control name" name="name" placeholder="Masukkan Nama" required>
                </div>
                <div class="form-group">
                    <label for="qty">QTY</label>
                    <input type="text" class="form-control qty" name="qty" placeholder="Masukkan QTY" required>
                </div>
                <div class="form-group">
                    <label for="image">Image</label>
                    <input type="file" accept=".jpeg,.jpg,.png" class="form-control" name="image">
                    <span>Pilih Gambar untuk update image</span>
                </div>
            </div>
            <div class="modal-footer">
                <input type="hidden" name="id" class="id"/>
                <input type="hidden" value="<%= itemId %>" name="itemId"/>
                <button type="button" class="btn btn-secondary" data-dismiss="modal">Cancel</button>
                <button type="submit" class="btn btn-primary">Update</button>
            </div>
        </form>
    </div>
</div>
</div>
  